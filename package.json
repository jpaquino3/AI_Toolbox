{
  "name": "ai-toolbox",
  "version": "1.0.0",
  "description": "An AI tools dashboard for managing and accessing various AI generation platforms",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "concurrently \"webpack serve --mode development\" \"cross-env NODE_ENV=development electron .\"",
    "build": "webpack --mode production",
    "package": "npm run build && electron-builder build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "appId": "com.aifm.toolbox",
    "productName": "AI Toolbox",
    "mac": {
      "category": "public.app-category.productivity",
      "target": "dmg",
      "icon": "AppIcon.icns"
    },
    "files": [
      "build/**/*",
      "main.js",
      "node_modules/**/*"
    ]
  },
  "keywords": [
    "ai",
    "dashboard",
    "midjourney",
    "runway",
    "pika",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^27.3.11",
    "electron-builder": "^24.6.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
